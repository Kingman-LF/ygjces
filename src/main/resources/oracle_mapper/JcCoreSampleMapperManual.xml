  <select id="selectWtArrearsDetails" resultType="com.xwkj.ygjces.controller.vo.WtArrearsDetailsVo">
    select WT_UNIT as wtUnit,sum(to_number(real_money)) as arrearsAmount from JC_CORE_SAMPLE t
    where 1=1  and isuse = '报告已发放' and sf_status != '已收费'
    <if test="wtUnit!=null and wtUnit!=''">
      and WT_UNIT LIKE CONCAT (CONCAT('%' , #{wtUnit}) , '%')
    </if>
    <if test="startTime != null">
      AND promise_date_in <![CDATA[ > ]]> #{startTime}
    </if>
    <if test="endTime != null">
      AND promise_date_in <![CDATA[ < ]]> #{endTime}
    </if>

    group by WT_UNIT
  </select>

  <select id="getItemsThatNotBoundToTheOrg" resultMap="ManualResultMap">
    select t.xm_id,t.custom_xm_name
    from JC_XTCS_XM t
    where t.category_id is not null
    <if test="list != null">
      and t.xm_id not in
      <foreach collection="list" item="id" open="(" separator="," close=")">
        #{id}
      </foreach>
    </if>

  </select>

  <select id="getSampleInfoByItemIds" resultMap="ManualResultMap2">
    select t.xm_id,t.xm_name,t.wt_unit,t.wt_date,t.pz_date, nvl(trunc(t.pz_date),trunc(sysdate))-trunc(a.wt_date) use_time
    from JC_CORE_SAMPLE t
    inner join jc_core_wt_info a on t.wt_id = a.wt_id
    where 1=1
    <if test="startTime != null">
      and t.wt_date <![CDATA[ > ]]> #{startTime}
    </if>
    <if test="endTime != null">
      and t.wt_date <![CDATA[ < ]]> #{endTime}
    </if>
    <if test="list != null">
      and t.xm_id in
      <foreach collection="list" item="id" open="(" separator="," close=")">
        #{id}
      </foreach>
    </if>
  </select>

  <select id="getProvisionalTimeByItemId" resultType="java.lang.String">
    select t.bg_date_in_value
    from JC_XTCS_XM t
    where t.xm_id = #{itemId}
  </select>

  <select id="getJcXtcsXmCount" resultType="Integer">
    select count(distinct t.wt_num) from JC_CORE_SAMPLE t where t.xm_id  in
    ( select t.xm_id from JC_XTCS_XM t where t.category_id  in
    (select t.category_id from JC_XTCS_XM_CATEGORY t start with t.category_id = #{categoryId}
    connect by prior t.category_id = t.pid)) and EXTRACT (YEAR FROM t.ch_date) = #{currentYear}
  </select>

  <select id="getJcXtcsXmNameAndCount" resultMap="jcXtcsXmWithSampleVo">
    select count(distinct t.wt_num) as xm_count,k.xm_name
    from JC_CORE_SAMPLE t left join JC_XTCS_XM k on t.xm_id = k.xm_id
    <where>
      <if test="start == null and end == null">
        and EXTRACT (YEAR FROM t.ch_date) = #{defaultDate}
      </if>
      <if test="xmName != null and xmName != ''">
        and t.xm_name like CONCAT( '%', CONCAT(#{xmName},'%'))
      </if>
      <if test="start != null">
        AND t.ch_date <![CDATA[ >= ]]> #{start}
      </if>
      <if test="end != null">
        AND t.ch_date <![CDATA[ <= ]]> #{end}
      </if>
    </where>
    group by t.xm_id,k.xm_name
  </select>

