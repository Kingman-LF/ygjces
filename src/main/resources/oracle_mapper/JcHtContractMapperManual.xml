  <select id="getContractList"  resultMap="BaseResultMap">
    SELECT * FROM jc_ht_contract jhc WHERE 1=1
    <if test="contractName != null and contractName != ''">
      AND jhc.CONTRACT_NAME like CONCAT (CONCAT('%' , #{contractName}) , '%')
    </if>
    <if test="startTime != null">
      AND jhc.QD_DATE <![CDATA[ > ]]> #{startTime}
    </if>
    <if test="endTime != null">
      AND jhc.QD_DATE <![CDATA[ < ]]> #{endTime}
    </if>
  </select>

  <select id="getContractListByOneMonth"  resultMap="BaseResultMap">
    SELECT * FROM jc_ht_contract jhc
    <where>
      <if test="startDate != null">
        AND jhc.QD_DATE <![CDATA[ >= ]]> #{startDate}
      </if>
      <if test="endDate != null">
        AND jhc.QD_DATE <![CDATA[ <= ]]> #{endDate}
      </if>
      <if test="contractName != null and contractName != ''">
        AND jhc.CONTRACT_NAME like CONCAT (CONCAT('%' , #{contractName}) , '%')
      </if>
    </where>
  </select>

  <select id="getContractQuantity" parameterType="com.xwkj.ygjces.model.JcHtContract" resultMap="ManualMap">
    select to_char(qd_date,'yyyy-mm') "date",count(*) "val"
    from jc_ht_contract
    <where>
      <if test="(startTime == null) and (endTime == null)">
        AND to_char(qd_date,'YYYY-MM') = to_char(sysdate,'YYYY-MM')
      </if>
      <if test="startTime != null">
        AND to_char(qd_date,'YYYY-MM') = to_char(#{startTime},'YYYY-MM')
      </if>
    </where>
    group by to_char(qd_date,'yyyy-mm')
  </select>
  <select id="getContractCreateTime" parameterType="com.xwkj.ygjces.model.JcHtContract" resultType="java.lang.String">
      SELECT  to_char(QD_DATE,'yyyy-mm') FROM jc_ht_contract
    <where>
      <if test="(startTime == null) and (endTime == null)">
        AND EXTRACT (YEAR FROM qd_date) =  EXTRACT (YEAR FROM sysdate)
      </if>
      <if test="startTime != null">
        AND QD_DATE <![CDATA[ >= ]]> #{startTime}
      </if>
      <if test="endTime != null">
        AND QD_DATE <![CDATA[ <= ]]> #{endTime}
      </if>
    </where>
  group by to_char(QD_DATE,'yyyy-mm') ORDER BY to_char(QD_DATE,'yyyy-mm') ASC
  </select>

